h1 Parties#edit

div#map

input#searchbox type='text'

javascript:
  $('document').on('page:load', init_map_place_selection)
  $('document').ready(init_map_place_selection)


=semantic_form_for @party do |f|
  =f.inputs :title, :type, :date
  =f.inputs do
    =f.input :summary, :label => false
  /temp
  =f.inputs :coord_latitude, :coord_longitude, hidden: true
  =f.actions

javascript:
  var CKEDITOR_BASEPATH = "/javascripts/ckeditor/"
  $.getScript("/javascripts/ckeditor/ckeditor.js", function() {CKEDITOR.replace( 'party[summary]' ); })
  jQuery.cachedScript = function( url, options ) {
    options = $.extend( options || {}, {
      dataType: "script",
      cache: true,
      url: url
    });
    return jQuery.ajax( options );
  };
  $.cachedScript( "ajax/test.js" )
  
